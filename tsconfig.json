{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node", "@playwright/test"]
  },
  "include": [
    "tests/**/*",
    "pages/**/*",
    "config/**/*",
    "utils/**/*",
    "fixtures/**/*"
, "ocr/ocrDigits.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
